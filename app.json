{
  "expo": {
    "name": "Joule Guns",
    "slug": "pcp-custom-app",
    "version": "0.0.18",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "icon": "assets/images/icone.png",
    "splash": {
      "resizeMode": "stretch",
      "image": "assets/images/icone.png",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.pcp.custom.app",
      "permissions": ["INTERNET"],
      "adaptiveIcon": {
        "foregroundImage": "assets/images/icone.png",
        "backgroundColor": "#000000"
      }
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "resizeMode": "stretch",
          "image": "assets/images/icone.png"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ef4106b6-4a63-47ea-8bd1-d80ce8b01068"
      }
    }
  }
}
