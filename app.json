{
  "expo": {
    "name": "PCP Custom APP",
    "slug": "pcp-custom-app",
    "icon": "./assets/images/icon.png",
    "version": "0.0.7",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffff"
    },
    "updates": {
      "enabled": true,
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.pcp.custom.app",
      "versionCode": 7,
      "adaptiveIcon": {
        "backgroundColor": "#1D1D1B"
      },
      "permissions": ["INTERNET"]
    }
  }
}
